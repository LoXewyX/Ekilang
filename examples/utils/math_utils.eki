fn double(x) {
    return x * 2
}

fn triple(x) {
    return x * 3
}

fn square(x) {
    return x * x
}

fn cube(x) {
    x * x * x
}

class MathHelper {
    fn factorial(self, n) {
        result = 1
        i = 2
        while i <= n {
            result = result * i
            i = i + 1
        }
        result
    }
}
